@model TestesBeneficios.Domain.Entidades.ProdutoFaixaEtaria

<div class="card">
    <div class="card-body">
        <h4 class="card-title">Produto Faixa Etaria</h4>
        <p class="card-description">Produto Faixa Etaria </p>
        <form class="forms-sample" asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-sample">
                <label asp-for="FaixaDe" class="control-label"></label>
                <input asp-for="FaixaDe" class="form-control" />
                <span asp-validation-for="FaixaDe" class="text-danger"></span>
            </div>
            <div class="form-sample">
                <label asp-for="FaixaAte" class="control-label"></label>
                <input asp-for="FaixaAte" class="form-control" />
                <span asp-validation-for="FaixaAte" class="text-danger"></span>
            </div>
            <div class="form-sample">
                <label asp-for="Preco" class="control-label"></label>
                <input asp-for="Preco" class="form-control" />
                <span asp-validation-for="Preco" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdProduto" class="control-label"></label>
                <select asp-for="IdProduto" class="form-control" asp-items="@ViewBag.ProdutoId"></select>
                <span asp-validation-for="IdProduto" class="text-danger"></span>
            </div>
            <div class="form-sample">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


