@model TestesBeneficios.Domain.DTO.SimulacaoDTO



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>

    <h3>Você tem certeza em excluir ?</h3>
    <div>
        <h4>Produtos</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Nome)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Nome)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Email)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Email)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Cpf)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Cpf)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Abrangencia)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Abrangencia)
            </dd>
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Deletar" class="btn btn-dark btn-rounded btn-fw" />
            <a asp-action="Index" type="button" class="btn btn-dark btn-rounded btn-fw">Voltar à Lista</a>
        </form>
    </div>
</body>
</html>
